<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/grey300"
    tools:context="com.myself.scenery.ui.base.HelpFeedbackActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/iv_aboutus_back"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="@dimen/dp_fifteen"
            android:background="@drawable/icon_back_white"/>
        <TextView
            android:id="@+id/tv_aboutus_title"
            android:layout_width="0dp"
            android:layout_height="66dp"
            android:text="关于我们"
            android:gravity="center"
            android:layout_weight="1"
            android:textColor="@color/white"
            android:textSize="22sp"/>
        <TextView
            android:id="@+id/tv_faboutus_switch"
            android:layout_width="wrap_content"
            android:layout_height="66dp"
            android:text="管理员"
            android:gravity="center"
            android:background="@color/colorPrimary"
            android:textColor="@color/white"
            android:visibility="gone"
            android:layout_marginRight="@dimen/dp_ten"
            android:textSize="16sp"/>
    </LinearLayout>
<TextView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:text="武夷山讲解"/>
</LinearLayout>
